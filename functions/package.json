    "onesignal-node": "^3.4.0"
  },
  "devDependencies": {
    "eslint": "^8.15.0",
    "eslint-config-google": "^0.14.0",
    "firebase-functions-test": "^3.1.0",
    "typescript": "^5.1.6",
    "@types/node": "^20.4.5"
  },
  "private": true
}
